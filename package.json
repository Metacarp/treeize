{
  "name": "treeize",
  "version": "1.2.1",
  "description": "Converts tabular row data (as from SQL joins, flat JSON, etc) to deep object graphs based on simple column naming conventions - without the use of an ORM or models.",
  "main": "./lib/treeize.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/kwhitley/treeize.git"
  },
  "keywords": [
    "JSON",
    "SQL",
    "tree",
    "object",
    "graph",
    "hydration",
    "model",
    "deep",
    "convert",
    "expand",
    "flat",
    "ORM"
  ],
  "author": "K. R. Whitley <contact@krwhitley.com> (http://krwhitley.com/)",
  "licenses": [
    {
      "type": "MIT",
      "url": "http://en.wikipedia.org/wiki/MIT_License"
    }
  ],
  "bugs": {
    "url": "https://github.com/kwhitley/treeize/issues"
  },
  "dependencies": {
    "lodash": "~1.3.1",
    "inflection": "~1.2.6"
  },
  "devDependencies": {
    "grunt": "latest",
    "grunt-cli": "latest",
    "nodeunit": "latest",
    "grunt-contrib-jshint": "latest",
    "grunt-contrib-nodeunit": "latest",
    "grunt-contrib-watch": "latest"
  },
  "scripts": {
    "test": "node_modules/.bin/grunt nodeunit"
  }
}
